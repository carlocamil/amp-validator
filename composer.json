{
    "name": "jeffochoa/amp-validator",
    "description": "Test your Accelerated Mobile Pages right away from your terminal.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli", "amp"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jeffochoa/amp-validator/issues",
        "source": "https://github.com/jeffochoa/amp-validator"
    },
    "authors": [
        {
            "name": "Jefferson Ochoa",
            "email": "jeffer.8a@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "kitetail/zttp": "dev-master",
        "laravel-zero/framework": "5.6.*",
        "league/csv": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/amp-validator"
    ]
}
